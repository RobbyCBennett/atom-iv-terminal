{
  "name": "iv-terminal",
  "main": "./lib/main",
  "version": "1.3.1",
  "description": "A simple terminal for panes and docks.",
  "keywords": [
    "command",
    "console",
    "prompt",
    "pty",
    "terminal",
    "term",
    "tty",
    "xterm"
  ],
  "repository": "https://github.com/samuelivarsson/atom-iv-terminal",
  "license": "MIT",
  "activationCommands": {
    "atom-text-editor": [
      "iv-terminal:insert-custom-text-1",
      "iv-terminal:insert-custom-text-2",
      "iv-terminal:insert-custom-text-3",
      "iv-terminal:insert-custom-text-4",
      "iv-terminal:insert-custom-text-5",
      "iv-terminal:insert-custom-text-6"
    ],
    "atom-workspace": [
      "iv-terminal:open"
    ]
  },
  "deserializers": {
    "TerminalSession": "deserializeTerminalSession"
  },
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "bundleDependencies": false,
  "dependencies": {
    "etch": "^0.14.0",
    "node-pty-prebuilt-multiarch": "^0.9.0",
    "rgb-hex": "^3.0.0",
    "xterm": "^4.2.0",
    "xterm-addon-fit": "^0.3.0"
  },
  "bugs": {
    "url": "https://github.com/samuelivarsson/atom-iv-terminal/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jasmine": true,
      "node": true
    },
    "globals": {
      "atom": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
